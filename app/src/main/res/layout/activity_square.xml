<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.SquareActivity">
    <ImageButton
        android:id="@+id/Back"
        android:layout_width="@dimen/x36"
        android:layout_height="@dimen/x64"
        android:background="@drawable/back"
        android:layout_marginEnd="@dimen/x72"
        app:layout_constraintEnd_toStartOf="@+id/tv"
        app:layout_constraintTop_toTopOf="@+id/tv"
        app:layout_constraintBottom_toBottomOf="@+id/tv"/>
    <TextView
        android:id="@+id/tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="歌单广场"
        android:textSize="18dp"
        android:textColor="#FF041748"
        android:layout_marginStart="@dimen/x250"
        android:layout_marginTop="@dimen/x120"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btn1"
        android:layout_width="@dimen/x236"
        android:layout_height="@dimen/x92"
        app:layout_constraintTop_toTopOf="@+id/btn3"
        app:layout_constraintBottom_toBottomOf="@+id/btn3"
        app:layout_constraintEnd_toStartOf="@+id/btn2">
        <ImageButton
            android:id="@+id/btn1_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/table_btn"/>
        <TextView
            android:id="@+id/tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="华语"
            android:textSize="15dp"
            android:textColor="#ff3077ff"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btn2"
        android:layout_width="@dimen/x236"
        android:layout_height="@dimen/x92"
        app:layout_constraintTop_toTopOf="@+id/btn3"
        app:layout_constraintBottom_toBottomOf="@+id/btn3"
        app:layout_constraintEnd_toStartOf="@+id/btn3">
        <ImageButton
            android:id="@+id/btn2_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/table_btn"/>
        <TextView
            android:id="@+id/tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="欧美"
            android:textSize="15dp"
            android:textColor="#ff3077ff"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btn3"
        android:layout_width="@dimen/x236"
        android:layout_height="@dimen/x92"
        android:layout_marginTop="@dimen/x60"
        app:layout_constraintTop_toBottomOf="@+id/tv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">
        <ImageButton
            android:id="@+id/btn3_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/table_btn"/>
        <TextView
            android:id="@+id/tv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="流行"
            android:textSize="15dp"
            android:textColor="#ff3077ff"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btn4"
        android:layout_width="@dimen/x236"
        android:layout_height="@dimen/x92"
        app:layout_constraintTop_toTopOf="@+id/btn3"
        app:layout_constraintBottom_toBottomOf="@+id/btn3"
        app:layout_constraintStart_toEndOf="@+id/btn3">
        <ImageButton
            android:id="@+id/btn4_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/table_btn"/>
        <TextView
            android:id="@+id/tv4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="古风"
            android:textSize="15dp"
            android:textColor="#ff3077ff"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btn5"
        android:layout_width="@dimen/x236"
        android:layout_height="@dimen/x92"
        app:layout_constraintTop_toTopOf="@+id/btn3"
        app:layout_constraintBottom_toBottomOf="@+id/btn3"
        app:layout_constraintStart_toEndOf="@+id/btn4">
        <ImageButton
            android:id="@+id/btn5_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/table_btn"/>
        <TextView
            android:id="@+id/tv5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="轻音乐"
            android:textSize="15dp"
            android:textColor="#ff3077ff"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.viewpager2.widget.ViewPager2
        android:id="@+id/ViewPaper"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/x36"
        app:layout_constraintTop_toBottomOf="@+id/btn3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>